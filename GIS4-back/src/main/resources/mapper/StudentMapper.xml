<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssj.gis4.mapper.StudentMapper">
    <select id="selectStudentById" parameterType="Long" resultType="com.ssj.gis4.domain.Student">
        select * from student where id = #{id}
    </select>


</mapper>